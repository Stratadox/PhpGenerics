{
    "timeout": 3,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "build\/logs\/infection-log.txt"
    },
    "mutators": {
        "@default": true,
        "NotIdenticalNotEqual": {
            "ignore": ["*"]
        },
        "IdenticalEqual": {
            "ignore": ["*"]
        }
    }
}
